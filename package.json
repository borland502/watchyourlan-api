{
  "name": "watchyourlan-api",
  "version": "0.2.1",
  "type": "module",
  "module": "./dist/index.mjs",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "direnv allow; npx tsx scripts/script-exec.mts --cmd=build",
    "lint": "npx tsx scripts/script-exec.mts --cmd=lint",
    "lint:fix": "npx tsx scripts/script-exec.mts --cmd=lint --fix",
    "format": "prettier --write \"src/**/*.mts\" \"src/**/*.ts\" \"src/**/*.tsx\" \"spec/**/*.mts\" \"spec/**/*.ts\" \"spec/**/*.tsx\"",
    "start": "npx tsx ./dist/index.mjs --env.NODE_ENV=production --serve",
    "start:cjs": "npx tsx ./dist/index.cjs --env.NODE_ENV=production --serve",
    "dev": "npx tsx --watch ./src/index.mts --env=development",
    "dev:debug": "npx tsx --inspect-brk --watch ./src/index.mts --env=development",
    "test": "jest --watch --coverage",
    "test:no-reloading": "jest --silent --coverage"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@sequelize/sqlite3": "^7.0.0-alpha.41",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-paginate": "^1.0.2",
    "helmet": "^7.1.0",
    "inserturlparams": "^2.0.4",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "node-config-ts": "^3.3.1",
    "prettier": "^3.3.3",
    "query-string": "^9.1.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "ts-command-line-args": "^2.5.1",
    "webpack-cli": "^5.1.4",
    "xdg-basedir": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/bluebird": "^3.5.42",
    "@types/chalk": "^0.4.31",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/express": "^4.17.21",
    "@types/express-paginate": "^1.0.4",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/host-validation": "^2.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jest": "^29.5.12",
    "@types/jsonfile": "^6.1.4",
    "@types/lodash": "^4.17.7",
    "@types/minimist": "^1.2.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.8",
    "@types/prettier": "^2.7.3",
    "@types/query-string": "^6.2.0",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@webpack-cli/generators": "^3.0.7",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "find": "^0.3.0",
